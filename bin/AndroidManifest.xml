<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ksoft.mynote"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/note_lunch"
        android:label="@string/app_name"
        android:theme="@style/MyTheme" >
         <activity
            android:name="com.ksoft.mynote.PasswordActivity"
            android:label="@string/title_activity_password" 
            
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.ksoft.mynote.HomeActivity"
            android:label="@string/app_name" >
           
        </activity>
         <activity
            android:name="com.ksoft.mynote.EnablePwdActivity"
            android:label="@string/title_activity_enable_pwd" >
            
             
        </activity>
       
        <activity
            android:name="com.ksoft.mynote.AddNoteActivity"
            android:label="@string/title_activity_add_note" >
        </activity>
        
        
        <activity
            android:name="com.ksoft.mynote.ResetPwdActivity"
            android:label="@string/title_activity_reset_pwd" >
        </activity>
       
        <activity
            android:name="com.ksoft.mynote.ShowNoteActivity"
            android:label="@string/title_activity_show_note" >
        </activity>
    </application>

</manifest>
